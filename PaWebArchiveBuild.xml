<?xml version="1.0" encoding="utf-8"?>
<BuildProcess ApplicationName="PA Web Archive Build">
	<PreBuildCommands>
		<Commands>
			<CommandItem Command="del">
				<Arg>$(RootBuildDir)\Installers\PA Package\Phonoloy Assistant 3.0.1 with .Net.exe</Arg>
				<Arg>$(RootBuildDir)\Installers\PA Package\Phonoloy Assistant 3.0.1 only.exe</Arg>
			</CommandItem>
			<CommandItem Command="md">
				<Arg>$(RootBuildDir)\Installers\PA Package\TempRoot</Arg>
			</CommandItem>
			<!-- Create the self-extractor that includes .Net 2.0 SP1 -->
			<CommandItem Command="md">
				<Arg>$(RootBuildDir)\Installers\PA Package\TempRoot\Phonology Assistant 3.0.1 with .Net</Arg>
			</CommandItem>
			<CommandItem Command="xcopy">
				<Arg>/E</Arg>
				<Arg>"$(RootBuildDir)\Installers\PA Package\With .NET 2.0 SP1\SIL_PA_30\*.*"</Arg>
				<Arg>"$(RootBuildDir)\Installers\PA Package\TempRoot\Phonology Assistant 3.0.1 with .Net"</Arg>
			</CommandItem>
			<CommandItem Command="$(RootBuildDir)\7za">
				<Arg>a</Arg>
				<Arg>-r</Arg>
				<Arg>-sfx7z.sfx</Arg>
				<Arg>-t7z</Arg>
				<Arg>-mx=9</Arg>
				<Arg>"$(RootBuildDir)\Installers\PA Package\Phonoloy Assistant 3.0.1 with .Net.exe"</Arg>
				<Arg>"$(RootBuildDir)\Installers\PA Package\TempRoot\*"</Arg>
				<Arg>-x!.svn</Arg>
				<Arg>-x!"autorun.*"</Arg>
			</CommandItem>
			<CommandItem Command="rd">
				<Arg>$(RootBuildDir)\Installers\PA Package\TempRoot\Phonology Assistant 3.0.1 with .Net</Arg>
			</CommandItem>
			<!-- Create the self-extractor without .Net 2.0 SP1 -->
			<CommandItem Command="md">
				<Arg>$(RootBuildDir)\Installers\PA Package\TempRoot\Phonology Assistant 3.0.1 only</Arg>
			</CommandItem>
			<CommandItem Command="xcopy">
				<Arg>/E</Arg>
				<Arg>"$(RootBuildDir)\Installers\PA Package\Without .NET 2.0\SIL_PA_30\*.*"</Arg>
				<Arg>"$(RootBuildDir)\Installers\PA Package\TempRoot\Phonology Assistant 3.0.1 only"</Arg>
			</CommandItem>
			<CommandItem Command="$(RootBuildDir)\7za">
				<Arg>a</Arg>
				<Arg>-r</Arg>
				<Arg>-sfx7z.sfx</Arg>
				<Arg>-t7z</Arg>
				<Arg>-mx=9</Arg>
				<Arg>"$(RootBuildDir)\Installers\PA Package\Phonoloy Assistant 3.0.1 only.exe"</Arg>
				<Arg>"$(RootBuildDir)\Installers\PA Package\TempRoot\*"</Arg>
				<Arg>-x!.svn</Arg>
				<Arg>-x!"autorun.*"</Arg>
			</CommandItem>
			<CommandItem Command="rd">
				<Arg>$(RootBuildDir)\Installers\PA Package\TempRoot</Arg>
			</CommandItem>
		</Commands>
	</PreBuildCommands>
</BuildProcess>