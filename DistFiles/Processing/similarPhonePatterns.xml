<?xml version="1.0" encoding="utf-8"?>
<inventory>
	<similarPhonePatterns>
		<pattern action="include" title="Alveolar nasal might assimilate to any place of articulation">
			<phone>
				<articulatoryFeatures>
					<feature>Alveolar</feature>
					<feature>Nasal</feature>
				</articulatoryFeatures>
			</phone>
			<phone>
				<articulatoryFeatures>
					<feature subclass="placeOfArticulation" />
					<feature>Nasal</feature>
				</articulatoryFeatures>
			</phone>
		</pattern>
		<pattern action="include" title="Glottal plosive has acoustic similarity to supralaryngeal voiceless plosives.">
			<phone>
				<articulatoryFeatures>
					<feature>Voiceless</feature>
					<feature>Glottal</feature>
					<feature>Plosive</feature>
				</articulatoryFeatures>
			</phone>
			<phone>
				<articulatoryFeatures>
					<feature>Voiceless</feature>
					<feature subclass="placeOfArticulation" />
					<feature>Plosive</feature>
				</articulatoryFeatures>
			</phone>
		</pattern>
		<pattern action="include" title="Voiceless glottal fricative has acoustic similarity to supralaryngeal voiceless fricatives.">
			<phone>
				<articulatoryFeatures>
					<feature>Voiceless</feature>
					<feature>Glottal</feature>
					<feature>Fricative</feature>
				</articulatoryFeatures>
			</phone>
			<phone>
				<articulatoryFeatures>
					<feature>Voiceless</feature>
					<feature subclass="placeOfArticulation" />
					<feature>Fricative</feature>
				</articulatoryFeatures>
			</phone>
		</pattern>
		<pattern action="exclude" title="Alveolar (coronal?) plosive and fricative are less similar than plosive and affricate.">
			<phone>
				<articulatoryFeatures>
					<feature>Alveolar</feature>
					<feature>Plosive</feature>
				</articulatoryFeatures>
			</phone>
			<phone>
				<articulatoryFeatures>
					<feature>Alveolar</feature>
					<feature>Fricative</feature>
				</articulatoryFeatures>
			</phone>
		</pattern>
		<pattern action="include" title="Voiceless plosive might become voiced tap/flap at the same place of articulation.">
			<phone>
				<articulatoryFeatures>
					<feature>Voiceless</feature>
					<feature>Plosive</feature>
				</articulatoryFeatures>
			</phone>
			<phone>
				<articulatoryFeatures>
					<feature>Voiced</feature>
					<feature>Tap/Flap</feature>
				</articulatoryFeatures>
			</phone>
		</pattern>
	</similarPhonePatterns>
</inventory>