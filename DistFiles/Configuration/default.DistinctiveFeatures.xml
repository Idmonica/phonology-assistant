<?xml version="1.0" encoding="utf-8"?>
<inventory>
	<featureOptions class="distinctive">
		<featureOption name="contour_affricate" value="false" />
		<featureOption name="contour_prenasalized" value="false" />
		<featureOption name="contour_diphthong" value="false" />
	</featureOptions>
	<featureDefinitions class="distinctive" title="From Introductory Phonology by Bruce Hayes. The radical feature and the feature values for clicks have been added.">
		<featureDefinition class="bivalent" category="major">
			<name>syll</name>
			<fullname>syllabic</fullname>
			<rule>
				<forAnyOf>
					<feature>consonant</feature>
				</forAnyOf>
				<ifAnyOf>
					<feature>syllabic</feature>
				</ifAnyOf>
				<then>
					<value>+</value>
				</then>
				<else>
					<value>-</value>
				</else>
			</rule>
			<rule>
				<forAnyOf>
					<feature>vowel</feature>
				</forAnyOf>
				<ifAnyOf>
					<feature>non-syllabic</feature>
				</ifAnyOf>
				<then>
					<value>-</value>
				</then>
				<else>
					<value>+</value>
				</else>
			</rule>
		</featureDefinition>
		<featureDefinition class="bivalent" category="major">
			<name>cons</name>
			<fullname>consonantal</fullname>
			<rule>
				<forAnyOf>
					<feature>plosive</feature>
					<feature>implosive</feature>
					<feature>click</feature>
					<feature>affricate</feature>
					<feature>nasal</feature>
					<feature>trill</feature>
					<feature>tap</feature>
					<feature>flap</feature>
					<features>
						<feature>fricative</feature>
						<notAnyOf>
							<feature>vowel</feature>
						</notAnyOf>
					</features>
				</forAnyOf>
				<ifAnyOf>
					<features>
						<feature>rounded</feature>
						<feature>velar</feature>
						<feature>fricative</feature>
					</features>
					<features>
						<feature>glottal</feature>
						<feature>fricative</feature>
					</features>
				</ifAnyOf>
				<then>
					<value>-</value>
				</then>
				<else>
					<value>+</value>
				</else>
			</rule>
			<rule>
				<forAnyOf>
					<feature>approximant</feature>
					<feature>vowel</feature>
				</forAnyOf>
				<ifAnyOf>
					<feature>lateral</feature>
					<feature>retroflex</feature>
				</ifAnyOf>
				<then>
					<value>+</value>
				</then>
				<else>
					<value>-</value>
				</else>
			</rule>
		</featureDefinition>
